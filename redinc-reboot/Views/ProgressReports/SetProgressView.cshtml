@model redinc_reboot.Models.ProgressReportModel

@{
    ViewBag.Title = "SetProgressView";
}

<h2>Problem Set Progress</h2>
<table>
@foreach(var setItem in Model.ProblemSetProgressList)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => setItem.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => setItem.IsSolved)
        </td>
        <td>
            @Html.DisplayFor(modelItem => setItem.AvgAttempts)
        </td>
        <td>
            @Html.DisplayFor(modelItem => setItem.NumCorrect)
        </td>
    </tr>
}
</table>