@model redinc_reboot.Models.ProgressReportModel

@{
    ViewBag.Title = "StudentProgressView";
}

<h2>Student Progress</h2>
<table>
@foreach(var setItem in Model.StudentProgressList)
{
    <tr>
        <td>
            @Html.ActionLink(setItem.Name, "SetProgressView", "ProgressReports")
        </td>
        <td>
            @Html.DisplayFor(modelItem => setItem.AvgAttempts)
        </td>
        <td>
            @Html.DisplayFor(modelItem => setItem.NumCorrect)
        </td>
    </tr>
}
</table>