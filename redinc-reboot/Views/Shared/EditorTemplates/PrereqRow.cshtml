@model core.Modules.ProblemSet.ProblemSetData
<tr class="set-row">
    @using(Html.BeginCollectionItem("Prereqs"))
    {
        @Html.HiddenFor(m => m.Id)
        @Html.HiddenFor(m => m.Name);
        <td class="set-td">
            @Html.ActionLink(Model.Name, "Edit", new { id = Model.Id })
        </td>
        <td class="set-td">
            @Html.TextBoxFor(m => m.PrereqCount)
        </td>
        <td class="set-td">
            <button class="deletePrereq">X</button>
        </td>
    }    
</tr>
